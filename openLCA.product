      startupMessageRect="7,500,445,20"
      startupForegroundColor="000000" />
   <launcher name="openLCA">
      <macosx icon="/olca-app/icons/logo.icns"/>
      <solaris/>
      <win useIco="false">
         <bmp
            winSmallHigh="/olca-app/icons/logo_16_32bit.bmp"
            winSmallLow="/olca-app/icons/logo_16_8bit.bmp"
            winMediumHigh="/olca-app/icons/logo_32_32bit.bmp"
            winMediumLow="/olca-app/icons/logo_32_8bit.bmp"
            winLargeHigh="/olca-app/icons/logo_48_32bit.bmp"
            winLargeLow="/olca-app/icons/logo_48_8bit.bmp"/>
      </win>
   </launcher>

   <intro introId="org.openlca.core.welcome.intro"/>

   <vm>
      <macos include="true">org.eclipse.jdt.launching.JRE_CONTAINER/org.eclipse.jdt.internal.debug.ui.launcher.StandardVMType/JavaSE-1.8</macos>
      <windows include="false">org.eclipse.jdt.launching.JRE_CONTAINER/org.eclipse.jdt.internal.debug.ui.launcher.StandardVMType/JavaSE-1.8</windows>
