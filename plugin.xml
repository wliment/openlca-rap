   <extension
         id="openLCA"
         point="org.eclipse.rap.ui.entrypoint">
      <product
            application="org.openlca.app.Application"
            name="openLCA">
         <property
               name="windowImages"
               value="icons/logo_16_32bit.png,icons/logo_32_32bit.png,icons/logo_48_32bit.png,icons/logo_64.png,icons/logo_128.png">
         </property>
         <property
               name="aboutText"
               value="openLCA 1.4.2&#x0A;&#x0A;Copyright (c) 2007 - 2015 GreenDelta. All rights reserved.&#x0A;Visit http://openlca.org and http://greendelta.com&#x0A;&#x0A;This product includes software developed in the openLCA &#x0A;project and other open source software.">
         </property>
         <property
               name="aboutImage"
               value="icons/about.png">
         </property>
         <property
               name="appName"
               value="openLCA">
         </property>
         <property
               name="introBrandingImage"
               value="product:icons/logo_glowing.png" />
         <property
               name="introBrandingImageText"
               value="openLCA" />
         <property
               name="startupProgressRect"
               value="0,360,540,5">
         </property>
         <property
               name="startupForegroundColor"
               value="000000">
         </property>
         <property
               name="startupMessageRect"
               value="7,500,445,20">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.viewer">
      <viewer
            viewerId="views.navigation">
            <options>
		        <property
		            name="org.eclipse.ui.navigator.hideLinkWithEditorAction"
		            value="true">
		         </property>
		         <property
		            name="org.eclipse.ui.navigator.hideCollapseAllAction"
		            value="true">
		         </property>
		         <property
		            name="org.eclipse.ui.navigator.hideAvailableCustomizationsDialog"
		            value="true">
		         </property>
			</options>           
      </viewer>
      <viewerContentBinding
            viewerId="views.navigation">
         <includes>
            <contentExtension
                  isRoot="true"
                  pattern="navigatorcontent.context">
            </contentExtension>
            <contentExtension
                  pattern="viewer.filter.emptycategory">
            </contentExtension>
            <contentExtension
                  pattern="viewer.filter.productflow">
            </contentExtension>
            <contentExtension
                  pattern="viewer.filter.elementaryflow">
            </contentExtension>
            <contentExtension
                  pattern="viewer.filter.wasteflow">
            </contentExtension>
         </includes>
      </viewerContentBinding>
      <viewerActionBinding
            viewerId="views.navigation">
         <includes>
            <actionExtension
                  pattern="actionprovider.navigation">
            </actionExtension>
         </includes>
      </viewerActionBinding>
      <dragAssistant
            class="org.openlca.app.navigation.NavigationDragAssistant"
            viewerId="views.navigation">
      </dragAssistant>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.navigatorContent">
      <navigatorContent
            activeByDefault="true"
            contentProvider="org.openlca.app.navigation.NavigationContentProvider"
            id="navigatorcontent.context"
            labelProvider="org.openlca.app.navigation.NavigationLabelProvider"
            name="%DataProvider"
            priority="normal">
         <triggerPoints>
            <instanceof
                  value="org.openlca.app.navigation.INavigationElement">
            </instanceof>
         </triggerPoints>
         <commonSorter
               class="org.openlca.app.navigation.NavigationSorter">
         </commonSorter>
         <dropAssistant
               class="org.openlca.app.navigation.NavigationDropAssistant"
               id="dropassistant.navigation">
            <possibleDropTargets>
               <instanceof
                     value="org.openlca.app.navigation.INavigationElement">
               </instanceof>
            </possibleDropTargets>
         </dropAssistant>
         <possibleChildren>
            <instanceof
                  value="org.openlca.app.navigation.INavigationElement">
            </instanceof>
         </possibleChildren>
      </navigatorContent>
      <actionProvider
            class="org.openlca.app.navigation.NavigationActionProvider"
            id="actionprovider.navigation"
            priority="normal">
         <enablement></enablement>
      </actionProvider>
      <commonFilter
            activeByDefault="false"
            class="org.openlca.app.navigation.filters.EmptyCategoryFilter"
            id="viewer.filter.emptycategory"
            name="%EmptyCategories">
      </commonFilter>
      <commonFilter
            activeByDefault="false"
            class="org.openlca.app.navigation.filters.ProductFlowFilter"
            id="viewer.filter.productflow"
            name="%ProductFlows">
      </commonFilter>
      <commonFilter
            activeByDefault="false"
            class="org.openlca.app.navigation.filters.ElementaryFlowFilter"
            id="viewer.filter.elementaryflow"
            name="%ElementaryFlows">
      </commonFilter>
      <commonFilter
            activeByDefault="false"
            class="org.openlca.app.navigation.filters.WasteFlowFilter"
            id="viewer.filter.wasteflow"
            name="%WasteFlows">
      </commonFilter>
   </extension>

   
   
